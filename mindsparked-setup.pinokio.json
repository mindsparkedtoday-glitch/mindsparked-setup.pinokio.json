{
  "name": "MindSparked Setup",
  "description": "Initial setup script for MindSparked automation in Pinokio",
  "version": "1.0.0",
  "run": [
    {
      "method": "git.clone",
      "params": {
        "repo": "https://github.com/mindsparkedtoday-glitch/mindsparked-main",
        "target": "C:\\Leo\\MindSparked"
      }
    },
    {
      "method": "shell.run",
      "params": {
        "command": "npm install",
        "cwd": "C:\\Leo\\MindSparked"
      }
    },
    {
      "method": "shell.run",
      "params": {
        "command": "npm start",
        "cwd": "C:\\Leo\\MindSparked"
      }
    }
  ]
}
